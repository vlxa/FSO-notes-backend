{
    "env": {
        "commonjs": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "semi": "off",
        "indent": [
            "error",
            2
        ],
        "no-underscore-dangle": "off"
    }
}